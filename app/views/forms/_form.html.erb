<center><h1> Registration_form</h1><%= form_for @form do |f| %>
<p>
<div class="form-group">
<%= f.label :firstname ,class: "col-sim-2 control-label"%>
<%= f.text_field :first_name,class: "form-control col-sm-10" %>

</div>
</p>
<p>
<%= f.label :lastname ,class: "col-md-4 control-label"%>
<%= f.text_field :last_name,class: "form-control col-mod-6" %>
</p>
<p>
<%= f.label :gender ,class: "col-md-4 control-label"%>
<%= f.radio_button :gender,'Male' %>Male
<%= f.radio_button :gender, 'Female'%>female
</p>
<p>
<%= f.label :address ,class: "col-md-4 control-label"%>
<%= f.text_area :address , :cols => "30", :rows => "4" ,class: "form-control col-mod-6"%>
</p>
<p>
<%= f.label :country,class: "col-md-4 control-label" %>
<%= f.select :country, ["india"],class: "form-control col-mod-6" %>
</p>
<%= f.submit "Submit" ,class: "btn btn-primary" %></center>
<% end %>
<hr>


